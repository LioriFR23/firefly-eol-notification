{
  "name": "firefly-eol-automation",
  "version": "1.0.0",
  "description": "Firefly End-of-Life (EOL) Automation Web App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "clean-start": "NODE_ENV=production node server.js",
